{
  "schemaVersion": 1,
  "id": "${name}",
  "version": "${version}",
  "name": "Bouquet",
  "description": "Essential hooks for Allium mods",
  "authors": [
      "hugeblank"
  ],
  "contact": {
      "homepage": "https://allium.hugeblank.dev/api",
      "sources": "https://github.com/hugeblank/bouquet"
  },
  "license": "MIT",
  "icon": "assets/bouquet/icon.png",
  "environment": "*",
  "custom": {
    "allium": {
      "entrypoints": {
        "static": "bouquet/init.lua",
        "mixin": "bouquet/mixin.lua"
      }
    }
  },
	"mixins": [
      "bouquet.mixins.json",
      {
        "config": "bouquet.client.mixins.json",
        "environment": "client"
      }
	],
	"depends": {
      "minecraft": "~26.1-",
      "fabricloader": ">=0.18.3",
      "java": ">=25"
	},
	"suggests": {
		"fabric-api": "*"
	}
}